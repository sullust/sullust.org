ACM_HOME=/opt/incharge/ACM/smarts

### Location of InfoVista

IV_HOME=/opt/InfoVista/Ivserver

IVD_HOME=/opt/InfoVista/VistaDiscovery

PO_HOME=/opt/InfoVista/Portal

CP_HOME=/opt/InfoVista/Cockpit

VM_HOME=/opt/InfoVista/Vistamart

### Location of smarts home for ESM

ESM_HOME=/opt/incharge/ESM/smarts



################################################################

if [ -z "$LOCAL_HOST" ]; then
   print "ERRCODE:16 - Failed to get hostname in var LOCAL_HOST"
   exit 16
fi

let D_CNT=${#DOMAINS[*]}

INDOMAINS=""
CURRDOM=""
let idx=0
while (( idx <= D_CNT ))
do
   let domidx=${DOMAINS[idx]}
   TMP=`echo ${DOMAIN[domidx]} | $GREP "${LOCAL_HOST}:"` 
   if [ "$TMP" != "" ]; then
      CURRDOM=${DOMAINS[idx]}
      if [ "$INDOMAINS" =  "" ]; then
         INDOMAINS=${CURRDOM}
      else
         INDOMAINS="${INDOMAINS} ${CURRDOM}"
      fi
   fi
   let idx=idx+1
done 

if [ -z "$INDOMAINS" ]; then
   print "ERRCODE:16 - Failed to locate $LOCAL_HOST definition in SMARTS_Config"
   exit 16
fi

######## Parse out all the servers for domain in which 
######## server $LOCAL_HOST is in

SM_MULTI_DOMAIN=""

MULTI[1]=SAM_MULTI_DOMAIN
MULTI[2]=AM_MULTI_DOMAIN
MULTI[3]=OI_MULTI_DOMAIN
MULTI[4]=PM_MULTI_DOMAIN
MULTI[5]=SIS_MULTI_DOMAIN
MULTI[6]=CONCORD_MULTI_DOMAIN
MULTI[7]=SNMCPE_MULTI_DOMAIN
MULTI[8]=DM_MULTI_DOMAIN
MULTI[9]=VOIP_MULTI_DOMAIN
MULTI[10]=IV_MULTI_DOMAIN
MULTI[11]=IVD_MULTI_DOMAIN

ALL[1]='ALL_SAMS'
ALL[2]='ALL_AMS'
ALL[3]='ALL_OIS'
ALL[4]='ALL_PMS'
ALL[5]='ALL_SISS'
ALL[6]='ALL_CONCORDS'
ALL[7]='ALL_SNMCPES'
ALL[8]='ALL_DMS'
ALL[9]='ALL_VOIPS'
ALL[10]='ALL_IVS'
ALL[11]='ALL_IVDS'

ALL_P[1]='ALL_SAM_PROCESSES'
ALL_P[2]='ALL_AM_PROCESSES'
ALL_P[3]='ALL_OI_PROCESSES'
ALL_P[4]='ALL_PM_PROCESSES'
ALL_P[5]='ALL_SIS_PROCESSES'
ALL_P[6]='ALL_CONCORD_PROCESSES'
ALL_P[7]='ALL_SNMCPE_PROCESSES'
ALL_P[8]='ALL_DM_PROCESSES'
ALL_P[9]='ALL_VOIP_PROCESSES'
ALL_P[10]='ALL_IV_PROCESSES'
ALL_P[11]='ALL_IVD_PROCESSES'

TYPE[1]=SAM
TYPE[2]=AM
TYPE[3]=OI
TYPE[4]=PM
TYPE[5]=SIS
TYPE[6]=CONCORD
TYPE[7]=SNMCPE
TYPE[8]=DM
TYPE[9]=VOIP
TYPE[10]=IV
TYPE[11]=IVD

set $INDOMAINS

INDOMS_CNT=$#

while test $# -ge 1
do
   
   SAVE_OARGS=$*
   domain=$1

   # SAMs
   TYPES[1]=`echo ${DOMAIN[domain]} | $AWK -F, '{ print $1 }'`
   # AMs
   TYPES[2]=`echo ${DOMAIN[domain]} | $AWK -F, '{ print $2 }' | $SED 's/^ *//g'`
   # OIs
   TYPES[3]=`echo ${DOMAIN[domain]} | $AWK -F, '{ print $3 }' | $SED 's/^ *//g'`
   # PMs
   TYPES[4]=`echo ${DOMAIN[domain]} | $AWK -F, '{ print $4 }' | $SED 's/^ *//g'`
   # SISs
   TYPES[5]=`echo ${DOMAIN[domain]} | $AWK -F, '{ print $5 }' | $SED 's/^ *//g'`
   # CONCORDs
   TYPES[6]=`echo ${DOMAIN[domain]} | $AWK -F, '{ print $6 }' | $SED 's/^ *//g'`
   # SNMCPEs
   TYPES[7]=`echo ${DOMAIN[domain]} | $AWK -F, '{ print $7 }' | $SED 's/^ *//g'`
   # DMs
   TYPES[8]=`echo ${DOMAIN[domain]} | $AWK -F, '{ print $8 }' | $SED 's/^ *//g'`
   # VOIPs
   TYPES[9]=`echo ${DOMAIN[domain]} | $AWK -F, '{ print $9 }' | $SED 's/^ *//g'`
   # IVs
   TYPES[10]=`echo ${DOMAIN[domain]} | $AWK -F, '{ print $10 }' | $SED 's/^ *//g'`
   # IVDs
   TYPES[11]=`echo ${DOMAIN[domain]} | $AWK -F, '{ print $11 }' | $SED 's/^ *//g'`

   let TYPES_CNT=${#TYPES[*]}

   PRIMARY_FLAG=0
   let PRIMARY=1
   let itr=1
   while (( $itr <= $TYPES_CNT ))
   do
      if [ -n "${TYPES[itr]}" ]; then
         set ${TYPES[itr]}
         SAVE_IARGS=
         let idx=0
         eval $( print DOM${domain}_${TYPE[itr]}${idx}_PROCESSES)=
         eval $( print ${MULTI[itr]})=
         eval $( print ${ALL_P[itr]})=
         eval $( print ${ALL[itr]})=
         while test $# -ge 1
         do
           let PRIMARY=PRIMARY%2
           SERVER=$1
           PROCESSES=`echo $1 | $GREP ':'`
           if [ -n "$PROCESSES" ]; then
              PROCESSES=`echo $1 | $SED 's/\:/ /g'`
              SAVE_IARGS=$*
              set $PROCESSES
              SERVER=$1
              shift
              #PROCESS=$1
              #PROCESS0=`print $1 | $SED 's/@[0-9]*//g'`
              #PROCESSES=`echo $* | $SED 's/^ //g;'`
              if (( $# == 0 )); then
                 # Just a server def without process info, so stuff into server
                 # vars ALL_AMS/ALL_SAMS/ ...
                 TMP1=`eval print '$'$( print ${ALL[itr]})`
                 if [ -n "$TMP1" ]; then
                     SAVE_PARGS=$*
                     set $TMP1
                     FOUND=0
                     while test $# -gt 0
                     do
                        PROC=`print $1 | $SED 's/ //g'`
                        if [ "$PROC" = "$SERVER" ]; then
                           FOUND=1
                        fi
                        # Don't break even if FOUND here since we want to return
                        # the number of set args to 0, so shift them on off
                        shift
                     done
 
                     if (( FOUND == 0 )); then
                        eval "${ALL[itr]}=\"`eval print '$'$( print ${ALL[itr]} )` `print ${SERVER}`\""
                     fi
                 else
                     eval "${ALL[itr]}=\"`print ${SERVER}`\""
                 fi
              fi
              while test $# -gt 0
              do
                PROCESS=$1
                PROCESS0=`print $1 | $SED 's/@[0-9]*//g'`
                if [ "$SERVER" = "$LOCAL_HOST" ]; then
                   PRIMARY_FLAG=$PRIMARY
                   #TMP0=`eval print "'$'$( print ${MULTI[itr]})"`
                   TMP0=`eval print '$'$( print ${MULTI[itr]})`
                   if [ -n "$TMP0" ]; then
                      SAVE_PARGS=$*
                      set $TMP0
                      FOUND=0
                      while test $# -gt 0
                      do
                       PROC=`print $1 | $SED 's/ //g'`
                       if [ "$PROC" = "$PROCESS0" ]; then
                            FOUND=1
                            break
                         fi
                         shift
                      done
 
                      set $SAVE_PARGS

                      if (( FOUND == 0 )); then
                         eval "${MULTI[itr]}=\"`print ${TMP0}` `print ${PROCESS0}`\""
                      fi
                   else
                      eval "${MULTI[itr]}=\"`print ${PROCESS0}`\""
                   fi
                fi
              #set $SAVE_IARGS
                TMP1=`eval print '$'$( print DOM${domain}_${TYPE[itr]}${idx}_PROCESSES)`
                SAVE_PARGS=$*
                if [ -n "$TMP1" ]; then
                   set $TMP1
                   FOUND=0
                   while test $# -gt 0
                   do
                       PROC=`print $1 | $SED 's/ //g'`
                       if [ "$PROC" = "$PROCESS" ]; then
                          FOUND=1
                          break
                       fi
                       shift
                   done

                   set $SAVE_PARGS

                   if (( FOUND == 0 )); then
                      eval "export DOM${domain}_${TYPE[itr]}${idx}_PROCESSES=\"`print ${TMP1}` `print ${PROCESS}`\""
                   fi
                else
                      eval "export DOM${domain}_${TYPE[itr]}${idx}_PROCESSES=\"`print ${PROCESS}`\""
                fi

                TMP1=`eval print '$'$( print ${ALL_P[itr]})`
                if [ -n "$TMP1" ]; then
                    SAVE_PARGS=$*
                    set $TMP1
                    FOUND=0
                    while test $# -gt 0
                    do
                       PROC=`print $1 | $SED 's/ //g'`
                       if [ "$PROC" = "$PROCESS" ]; then
                          FOUND=1
                          break
                       fi
                       shift
                    done

                    set $SAVE_PARGS

                    if (( FOUND == 0 )); then
                       eval "${ALL_P[itr]}=\"`eval print '$'$( print ${ALL_P[itr]} )` `print ${PROCESS}`\""
                    fi
                else
                      eval "${ALL_P[itr]}=\"`print ${PROCESS}`\""
                fi

                eval "export DOM${domain}_${TYPE[itr]}${idx}=\"${SERVER}\""

                TMP1=`eval print '$'$( print ${ALL[itr]})`
                if [ -n "$TMP1" ]; then
                    SAVE_PARGS=$*
                    set $TMP1
                    FOUND=0
                    while test $# -gt 0
                    do
                       PROC=`print $1 | $SED 's/ //g'`
                       if [ "$PROC" = "$SERVER" ]; then
                          FOUND=1
                          break
                       fi
                       shift
                    done

                    set $SAVE_PARGS

                    if (( FOUND == 0 )); then
                       eval "${ALL[itr]}=\"`eval print '$'$( print ${ALL[itr]} )` `print ${SERVER}`\""
                    fi
                else
                    eval "${ALL[itr]}=\"`print ${SERVER}`\""
                fi
                shift
              done
              set $SAVE_IARGS
           fi
           let idx=idx+1
           shift
           let PRIMARY=PRIMARY+1
         done
      fi
      let itr=itr+1
      let PRIMARY=1
   done

   set $SAVE_OARGS
   shift

done

######################## Done parsing domain servers ###################

OI_DNS_SERVERS=${OI_DNS[CURRDOM]}
