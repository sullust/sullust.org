   if [ -z "$ARCHIVE_INCL" ]; then
      ARCHIVE_INCL=`print $ALL_AM_PROCESSES | $SED 's/@[0-9]*//g'`
   else
      ARCHIVE_INCL=`print $ARCHIVE_INCL $ALL_AM_PROCESSES | $SED 's/@[0-9]*//g'`
   fi
fi
TMP=
