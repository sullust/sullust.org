# multiple domains ( like a SAM server), these vars are meaningless

CONCORD1=`print ${ALL_CONCORDS} | $AWK '{ print $1 }'`

# Set AM1 and AM2 legacy variables.  The method for setting them requires
# that the servers are always paired together sequentially in the DOMAIN 
# array entry.
# dlh - when localhost is the secondary server AM2 wrongly gets set to next
#       primary server in list. Using PREV to store and set correctly.

AM1=""
AM2=""
PREV=""

if [ -n "$ALL_AMS" ]; then
   set $ALL_AMS
   while test $# -gt 0
   do
      if [ "$1" = "$LOCAL_HOST" ]; then
         AM1=$1
         if [ "$PRIMARY_FLAG" = "1" ]; then
            AM2=$2
         else
            AM2=$PREV
         fi
         break
      else
         PREV=$1
         shift
      fi
   done
   # if LOCAL_HOST was not found above in ALL_AMS, then we are dealing with
   # a stand-alone VOIP.

   if [ -n "$ALL_VOIPS" ]; then
       set $ALL_VOIPS
       while test $# -gt 0
       do
          if [ "$1" = "$LOCAL_HOST" ]; then
             AM1=$1
             AM2=$2
             break
          else
             shift
          fi
       done
   fi
fi

